    {
      "cell_type": "code",
      "source": [
        "def encode(data, col, max_val):\n",
        "    data[col + '_Sin'] = np.cos(2 * np.pi * data[col]/max_val)\n",
        "    return data\n",
        "Legal_illegal = encode(Legal_illegal, 'Week_Day', 7)\n",
        "Legal_illegal = encode(Legal_illegal, 'Month', 12)\n",
        "Legal_illegal\n"
      ],
