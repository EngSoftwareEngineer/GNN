        "# Extract the node features\n",
        "x = df.to_numpy()\n",
        "# Extract the edges\n",
        "edge_index = torch.tensor(list(df.values), dtype=torch.long)\n",
        "# Create the graph data\n",
        "data = Data(x=x, edge_index=edge_index)\n",
        "#network\n",
        "G = nx.Graph()\n",
        "G.add_nodes_from(x[2])\n",
